{
  "kind": "collectionType",
  "collectionName": "taxi_drivers",
  "info": {
    "singularName": "taxi-driver",
    "pluralName": "taxi-drivers",
    "displayName": "Taxi Driver",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "profileImg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::language.language",
      "inversedBy": "taxi_drivers"
    },
    "socialLinkls": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "helpers.social-media"
    },
    "preferences": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "helpers.string-array"
    },
    "schedule": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "components.work-schedule"
    },
    "totalComments": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "default": 0,
      "required": true
    },
    "averageRating": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "float",
      "required": true,
      "default": 0
    },
    "comments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::comment.comment",
      "inversedBy": "taxi_drivers"
    },
    "car_class": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::car-class.car-class",
      "inversedBy": "taxi_drivers"
    },
    "carName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "carModel": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "passengersNum": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true
    }
  }
}
