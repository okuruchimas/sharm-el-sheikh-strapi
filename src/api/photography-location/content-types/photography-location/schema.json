{
  "kind": "collectionType",
  "collectionName": "photography_locations",
  "info": {
    "singularName": "photography-location",
    "pluralName": "photography-locations",
    "displayName": "Photography Location",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "location": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "position": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "helpers.position",
      "required": true
    },
    "about": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks"
    },
    "totalComments": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true,
      "default": 0
    },
    "averageRating": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "float",
      "required": true,
      "default": 0,
      "max": 5,
      "min": 0
    },
    "photographers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::photographer.photographer",
      "inversedBy": "photography_locations"
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "studio",
        "location"
      ],
      "default": "studio",
      "required": true
    }
  }
}
