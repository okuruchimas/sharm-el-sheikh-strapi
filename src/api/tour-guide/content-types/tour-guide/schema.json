{
  "kind": "collectionType",
  "collectionName": "tour_guides",
  "info": {
    "singularName": "tour-guide",
    "pluralName": "tour-guides",
    "displayName": "Tour Guide"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "profileImg": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::language.language",
      "inversedBy": "tour_guides"
    },
    "totalComments": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true,
      "default": 0
    },
    "averageRating": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "float",
      "required": true,
      "default": 0
    },
    "tours": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "comments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::comment.comment",
      "inversedBy": "tour_guides"
    }
  }
}
